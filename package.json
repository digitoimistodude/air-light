{
  "name": "air-light",
  "version": "9.6.1",
  "description": "A minimalist WordPress starter theme.",
  "author": "Digitoimisto Dude Oy (moro@dude.fi)",
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@double-great/stylelint-a11y": "^3.4.0",
    "@hint/hint-axe": "^4.4.21",
    "@hint/hint-button-type": "^3.0.23",
    "@hint/hint-compat-api": "^4.5.8",
    "@parcel/transformer-sass": "^2.15.4",
    "@wordpress/babel-preset-default": "^8.29.0",
    "@wordpress/scripts": "^30.22.0",
    "browser-sync": "^3.0.4",
    "caniuse-lite": "^1.0.30001737",
    "concurrently": "^9.2.1",
    "cssnano": "^7.1.1",
    "cssnano-preset-advanced": "^7.0.9",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "hint": "^7.1.13",
    "husky": "^9.1.7",
    "npm-check-updates": "^18.0.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.15.4",
    "postcss": "^8.5.6",
    "postcss-calc": "^10.1.1",
    "postcss-scss": "^4.0.9",
    "sass": "^1.91.0",
    "stylelint": "^16.23.1",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-recommended-scss": "^16.0.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "stylelint-rem-over-px": "^1.0.2",
    "stylelint-scss": "^6.12.1",
    "stylelint-value-no-unknown-custom-properties": "^6.0.1"
  },
  "scripts": {
    "prepare": "husky && chmod +x .husky/pre-commit .husky/commit-msg",
    "dev": "concurrently --kill-others -n \"CSS,JS,SYNC\" \"PARCEL_CONFIG=.parcelrc.watch POSTCSS_CONFIG=.postcssrc.dev parcel watch sass/global.scss sass/gutenberg-editor-styles.scss --dist-dir assets --no-hmr --no-cache --no-source-maps --log-level verbose\" \"parcel watch js/src/front-end.js js/src/gutenberg-editor.js --dist-dir assets/js --no-hmr --no-cache --no-source-maps\" \"npm run browsersync\"",
    "browsersync": "node parcel/browsersync.js",
    "build": "npm-run-all build:styles build:js",
    "build:styles": "parcel build sass/global.scss sass/gutenberg-editor-styles.scss --dist-dir assets --no-cache",
    "build:js": "parcel build js/src/front-end.js js/src/gutenberg-editor.js --dist-dir assets/js --no-cache",
    "dev:styles": "POSTCSS_CONFIG=.postcssrc.dev parcel build sass/global.scss sass/gutenberg-editor-styles.scss --dist-dir assets --no-optimize --no-cache",
    "dev:js": "parcel build js/src/front-end.js js/src/gutenberg-editor.js --dist-dir assets/js --no-optimize --no-cache",
    "lint": "eslint js/src",
    "lint:styles": "stylelint sass/**/*.scss",
    "phpcs": "phpcs"
  },
  "dependencies": {
    "core-js": "^3.45.1",
    "moveto": "^1.8.2",
    "reframe.js": "^4.0.2",
    "vnu-jar": "^24.10.17"
  }
}
